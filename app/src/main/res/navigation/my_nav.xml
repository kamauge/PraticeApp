<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/listUsersFragment2">
    <fragment
        android:id="@+id/listUsersFragment2"
        android:name="com.regera.aad.ListUsersFragment"
        android:label="list"
        tools:layout="@layout/fragment_list_users" >
        <action
            android:id="@+id/action_listUsersFragment2_to_addUserFragment2"
            app:destination="@id/addUserFragment2" />
        <action
            android:id="@+id/action_listUsersFragment2_to_updateUserFragment"
            app:destination="@id/updateUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/addUserFragment2"
        android:name="com.regera.aad.AddUserFragment"
        android:label="add"
        tools:layout="@layout/fragment_add_user" >
        <action
            android:id="@+id/action_addUserFragment2_to_listUsersFragment2"
            app:destination="@id/listUsersFragment2" />
    </fragment>
    <fragment
        android:id="@+id/updateUserFragment"
        android:name="com.regera.aad.UpdateUserFragment"
        android:label="fragment_update_user"
        tools:layout="@layout/fragment_update_user" >
        <action
            android:id="@+id/action_updateUserFragment_to_listUsersFragment2"
            app:destination="@id/listUsersFragment2" />
        <argument
            android:name="currentUser"
            app:argType="com.regera.aad.data.User" />
    </fragment>
</navigation>